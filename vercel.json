{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/scrape",
        "methods": ["POST"],
        "dest": "/server.js"
      },
      {
        "src": "/download",
        "methods": ["POST"],
        "dest": "/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ]
  }